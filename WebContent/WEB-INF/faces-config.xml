		</managed-property>
		<managed-property>
			<property-name>accessLogBean</property-name>
			<property-class>edu.uic.ids.model.AccessLogBean</property-class>
			<value>#{accessLogBean}</value>
		</managed-property>
	<managed-property>
			<property-name>statsBean</property-name>
			<property-class>edu.uic.ids.model.StatsBean</property-class>
			<value>#{statsBean}</value>
		</managed-property>
		
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>databaseOperations</managed-bean-name>
		<managed-bean-class>edu.uic.ids.action.DatabaseOperations</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.action.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>dbBean</property-name>
			<property-class>edu.uic.ids.model.DatabaseAccessInfoBean</property-class>
			<value>#{dbBean}</value>
		</managed-property>
		
	</managed-bean>
	
	

	<managed-bean>
		<managed-bean-name>accessLogBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.AccessLogBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
