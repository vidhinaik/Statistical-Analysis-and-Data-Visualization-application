	<managed-bean>
		<managed-bean-name>dbBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.DatabaseAccessInfoBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>dbAccess</managed-bean-name>
		<managed-bean-class>edu.uic.ids.action.DbAccess</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbBean</property-name>
			<property-class>edu.uic.ids.model.DatabaseAccessInfoBean</property-class>
			<value>#{dbBean}</value>
		</managed-property>
	</managed-bean>

	<managed-bean>
		<managed-bean-name>methods</managed-bean-name>
		<managed-bean-class>edu.uic.ids.action.Methods</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbBean</property-name>
			<property-class>edu.uic.ids.model.DatabaseAccessInfoBean</property-class>
			<value>#{dbBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.action.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>databaseOperations</property-name>
			<property-class>edu.uic.ids.action.DatabaseOperations</property-class>
