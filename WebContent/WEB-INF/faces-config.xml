		</managed-property>
		<managed-property>
			<property-name>dbBean</property-name>
			<property-class>edu.uic.ids.model.DatabaseAccessInfoBean</property-class>
			<value>#{dbBean}</value>
		</managed-property>
		
		<managed-property>
			<property-name>accessLogBean</property-name>
			<property-class>edu.uic.ids.model.AccessLogBean</property-class>
			<value>#{accessLogBean}</value>
		</managed-property>
