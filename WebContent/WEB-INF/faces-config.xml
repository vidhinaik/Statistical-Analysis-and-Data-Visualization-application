			<property-class>edu.uic.ids.action.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>dbBean</property-name>
			<property-class>edu.uic.ids.model.DatabaseAccessInfoBean</property-class>
			<value>#{dbBean}</value>
		</managed-property>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>mathManagedBean</managed-bean-name>
		<managed-bean-class>edu.uic.ids.model.MathManagedBean</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.action.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
