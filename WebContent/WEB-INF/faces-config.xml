		</managed-property>
	</managed-bean>

	

	<managed-bean>
		<managed-bean-name>reports</managed-bean-name>
		<managed-bean-class>edu.uic.ids.action.Reports</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
		<managed-property>
			<property-name>dbAccess</property-name>
			<property-class>edu.uic.ids.action.DbAccess</property-class>
			<value>#{dbAccess}</value>
		</managed-property>
		<managed-property>
			<property-name>statsManagedBean</property-name>
			<property-class>edu.uic.ids.model.StatsManagedBean</property-class>
			<value>#{statsManagedBean}</value>
		</managed-property>
		<managed-property>
			<property-name>dbBean</property-name>
			<property-class>edu.uic.ids.model.DatabaseAccessInfoBean</property-class>
			<value>#{dbBean}</value>
		</managed-property>
	</managed-bean>

	<navigation-rule>
		<from-view-id>/dba.jsp</from-view-id>
		<navigation-case>
			<from-outcome>SUCCESS</from-outcome>
			<to-view-id>/dbOptions.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/confirm.jsp</from-view-id>
		<navigation-case>
			<from-outcome>LOGOUT</from-outcome>
			<to-view-id>/logout.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>

</faces-config>
